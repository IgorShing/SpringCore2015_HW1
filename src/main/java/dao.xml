<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p = "http://www.springframework.org/schema/p"
	xmlns:c = "http://www.springframework.org/schema/c"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
 		http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
 		http://www.springframework.org/schema/util
		http://www.springframework.org/schema/util/spring-util-3.2.xsd">
		
	<import resource = "model.xml" />

	<bean id="usersDaoMap" class="com.cinema.manager.controller.dao.UsersDaoMapImpl">
		<constructor-arg ref = "users" />
	</bean>
	
	<bean id="auditoriumDao" class="com.cinema.manager.controller.dao.AuditoriumDaoImpl">
		<constructor-arg ref = "auditoriums" />
	</bean>
	
	<bean id="eventDao" class="com.cinema.manager.controller.dao.EventDaoImpl">
		<constructor-arg ref = "events" />
	</bean>
	
	<bean id="bookingDao" class="com.cinema.manager.controller.dao.BookingDaoImpl" />
	
	<bean id="ticketDao" class="com.cinema.manager.controller.dao.TicketDaoImpl">
		<constructor-arg ref = "tickets" />
	</bean>
	
</beans>